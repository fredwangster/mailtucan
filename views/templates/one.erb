
<html>
<head>
	<meta content="text/html; charset=utf-8" http-equiv="Content-Type" />
	<title>Email Template - Classic</title>
	<style type="text/css">
	a:hover { text-decoration: underline !important; }
	</style>
</head>

<body marginheight="0" topmargin="0" marginwidth="0" style="margin: 0px; background-color: #f7f2e4;" bgcolor="#f7f2e4" leftmargin="0">
	<!--100% body table-->
	<table cellspacing="0" border="0" cellpadding="0" width="100%" bgcolor="#f7f2e4">
		<tr>
			<td>
				<!--top links-->
				<table width="100%" border="0" cellspacing="0" cellpadding="0">
					<tr>
						<td valign="middle" align="center" height="45">
							<p style="font-size: 14px; line-height: 24px; font-family: Georgia, 'Times New Roman', Times, serif; color: #b0a08b; margin: 0px;">
								Is this email not displaying correctly? <webversion style="color: #bc1f31; text-decoration: none;" href="#">Try the web version.</webversion></p></td>
							</tr>
						</table>
						<!--header-->
						<table style="background:http://pmbcreative.com/newsletters/Template1/header-bg.jpg); background-repeat: no-repeat; background-position: center; background-color: #fffdf9;" width="684" border="0" align="center" cellpadding="0" cellspacing="0">
							<tr>
								<td>
									<table width="100%" border="0" cellspacing="0" cellpadding="0">
										<tr>
											<td valign="top" width="173">
												<!--ribbon-->
												<table border="0" cellspacing="0" cellpadding="0">
													<tr>
														<td height="120" width="45"></td>
														<td background="http://pmbcreative.com/newsletters/Template1/ribbon.jpg" valign="top" bgcolor="#c72439" height="120" width="80">
															<table width="100%" border="0" cellspacing="0" cellpadding="0">
																<tr>
																	<td valign="bottom" align="center" height="35">
																		<p style="font-size: 14px; font-family: Georgia, 'Times New Roman', Times, serif; color: #ffffff; margin-top: 0px; margin-bottom: 0px;">ISSUE</p>
																	</td>
																</tr>
																<tr>
																	<td valign="top" align="center">
																		<p style="font-size: 36px; font-family: Georgia, 'Times New Roman', Times, serif; color: #ffffff; margin-top: 0px; margin-bottom: 0px; text-shadow: 1px 1px 1px #333;"><%=@issue_number %></p>
																	</td>
																</tr>
															</table>
														</td>
													</tr>
												</table><!--ribbon-->
											</td>
											<td valign="middle" width="493"><table width="100%" border="0" cellspacing="0" cellpadding="0">
												<tr>
													<td height="60"></td>
												</tr>
												<tr>
													<td>
														<h1 style="text-align: center; color: #333333; margin-top: 0px; margin-bottom: 0px; font-weight: normal; font-size: 48px; font-family: Georgia, 'Times New Roman', Times, serif"><%=@pageData["name"] %><br><em>Newsletter</em></h1>
													</td>
												</tr>
												<tr>
													<td height="40">
													</td>
												</tr>
											</table>
											<!--date-->
											<table border="0" cellspacing="0" cellpadding="0">
												<tr>
													<td valign="top" align="center" bgcolor="#312c26" background="http://pmbcreative.com/newsletters/Template1/date-bg.jpg" width="357" height="42">
														<table width="100%" border="0" cellspacing="0" cellpadding="0"><tr><td height="5"></td></tr></table>
														<p style="font-size: 24px; font-family: Georgia, 'Times New Roman', Times, serif; color: #ffffff; margin-top: 0px; margin-bottom: 0px;">Week of <%=@newsletter_date %></p>
													</td>
												</tr>
											</table><!--/date-->
										</td>
										<td width="18"></td>
									</tr>
								</table>
							</td>
						</tr>
					</table><!--/header-->
					<!--email container-->
					<table bgcolor="#fffdf9" cellspacing="0" border="0" align="center" cellpadding="30" width="684">
						<tr>
							<td>
								<!--email content-->
								<table cellspacing="0" border="0" id="email-content" cellpadding="0" width="624">
									<tr>
										<td>
											<!--section 3-->
											<table width="100%" border="0" cellspacing="0" cellpadding="0">
												<tr>
													<td height="30"></td>
												</tr>
											</table>
											<table cellspacing="0" border="0" cellpadding="0" width="100%">
												<tr>
													<td valign="top" width="39%">
															<table width="220" border="0" align="left" cellpadding="0" cellspacing="0">
																<tr>
																	<td>
																		<table align="left" width="220" border="0" cellspacing="0" cellpadding="0">
																		<tr>
																			<td bgcolor="#312c26" valign="top" style="background-color: #312c26; border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; -khtml-border-radius: 4px;">
																				<table width="100%" border="0" cellspacing="0" cellpadding="20">
																					<tr>
																						<td>
																							<p style="font-size: 16px; font-family: Georgia, 'Times New Roman', Times, serif; color: #ffffff; margin-top: 0px; margin-bottom: 0px; font-weight: bold;">Upcoming Events:</p>
																							<table width="100%" border="0" cellpadding="0" cellspacing="0">
																								<tr>
																									<td height="10"></td>
																								</tr>
																							</table>
																						</td>
																					</tr>
																				</table>
																			</td>
																		</tr>
																		</table>
																	</td>
																</tr>
																<tr>
																	<td>
																	</td>
																</tr>
																<% @pageData["events"]["data"].each do |event| %>
																	<tr>
																	<td valign="top">
																		<!--break-->
																		<table width="100%" border="0" cellspacing="0" cellpadding="0">
																			<tr>
																				<td height="40"></td>
																			</tr>
																		</table><!--/break-->
																		<p style="font-size: 18px; font-family: Georgia, 'Times New Roman', Times, serif; color: #333333; margin-top: 0px; margin-bottom: 0px; font-weight: bold;"><%= event["name"] %></p>
																		<p style="font-size: 18px; font-family: Georgia, 'Times New Roman', Times, serif; color: #606060; margin-top: 0px; margin-bottom: 0px;"><%= event["start_time"] %></p>
																		<table width="100%" border="0" cellpadding="0" cellspacing="0">
																			<tr>
																				<td height="10"></td>
																			</tr>
																		</table>
																		<p style="font-size: 16px; line-height: 18px; font-family: Georgia, 'Times New Roman', Times, serif; color: #333; margin: 0px;"><%= event["location"] %></p>

																	</td>
																</tr>
																<% end %>
															</table>
															
													</td>
													<td valign="top" width="61%">
														<table width="100%" border="0" cellspacing="0" cellpadding="0">
															<tr>
																<td>
																	<img src="http://pmbcreative.com/newsletters/Template1/line-break-4.jpg" width="378" height="27">
																</td>
															</tr>
															<tr>
																<td align="center">
																	<p style="font-size: 24px; font-family: Georgia, 'Times New Roman', Times, serif; color: #bc1f31; margin-top: 0px; margin-bottom: 0px; font-weight: bold;">Latest News</p>
																</td>
															</tr>
															<tr>
																<td valign="top" height="40"><img src="http://pmbcreative.com/newsletters/Template1/line-break-5.jpg" width="378" height="17"></td>
															</tr>
														</table>
														<% @pageData["posts"]["data"].each do |feed| %>
														<h1 style="font-size: 24px; font-family: Georgia, 'Times New Roman', Times, serif; color: #333; margin-top: 0px; margin-bottom: 12px;"><%= feed["created_time"] %></h1>
														<img style="border: solid 1px #FFF; box-shadow: 2px 2px 6px #333; -webkit-box-shadow: 2px 2px 6px #333333; -khtml-box-shadow: 2px 2px 6px #333; -moz-box-shadow: 2px 2px 6px #333;" src="<% if feed["picture"] %><%= feed["picture"] %><% end%>" width="370" height="auto">
													
														<table width="100%" border="0" cellpadding="0" cellspacing="0">
															<tr>
																<td height="10"></td>
															</tr>
														</table>
													
														<p style="font-size: 16px; line-height: 22px; font-family: Georgia, 'Times New Roman', Times, serif; color: #333; margin: 0px; max-width: 370px; word-wrap:break-word;"><%= feed["message"] %></p>
														<table width="100%" border="0" cellspacing="0" cellpadding="0">
															<tr>
																<td><img src="http://pmbcreative.com/newsletters/Template1/line-break-6.jpg" width="378" height="40"></td>
															</tr>
														</table>
														<% end %>
													</td>
												</tr>
											</table><!--/section 3-->
														<!--line break-->
														<table cellspacing="0" border="0" cellpadding="0" width="100%">
															<tr>
																<td height="72"><img src="http://pmbcreative.com/newsletters/Template1/line-break-2.jpg" width="622" height="72">
																</td>
															</tr>
														</table><!--/line break-->
													</td>
												</tr>
											</table><!--/email content-->
										</td>
									</tr>
								</table><!--/email container-->
								<!--footer-->
								<table width="680" border="0" align="center" cellpadding="30" cellspacing="0">
									<tr>
										<td valign="top">
											<p style="font-size: 14px; line-height: 24px; font-family: Georgia, 'Times New Roman', Times, serif; color: #b0a08b; margin: 0px;">
												Youâ€™re receiving this newsletter because youâ€™ve subscribed to our newsletter<br> 
												Not interested anymore? <unsubscribe style="color: #bc1f31; text-decoration: none;" href="#">Unsubscribe instantly.</unsubscribe></p>
											</td>
											<td valign="top"><p style="font-size: 14px; line-height: 24px; font-family: Georgia, 'Times New Roman', Times, serif; color: #b0a08b; margin: 0px;">
												<% if @pageData["location"]["street"] %><%= @pageData["location"]["street"] %><% end %>, <% if @pageData["location"]["city"] %><%= @pageData["location"]["city"] %><% end %>, <% if @pageData["location"]["state"] %><%= @pageData["location"]["state"] %><% end %><br>
												<% if @pageData["location"]["country"] %><%= @pageData["location"]["country"] %><% end %></p></td>
											</tr>
											<tr>
												<td height="30"></td>
												<td height="30"></td>
											</tr>
										</table><!--/footer-->
									</td>
								</tr>
							</table><!--/100% body table-->
						</body>
						</html>