																				<table width="100%" border="0" cellspacing="0" cellpadding="20">
																					<tr>
																						<td>
																							<p style="font-size: 16px; font-family: Georgia, 'Times New Roman', Times, serif; color: #ffffff; margin-top: 0px; margin-bottom: 0px; font-weight: bold;">Upcoming Events:</p>
																							<table width="100%" border="0" cellpadding="0" cellspacing="0">
																								<tr>
																									<td height="10"></td>
																								</tr>
																							</table>
																						</td>
																					</tr>
																				</table>
																			</td>
																		</tr>
																		</table>
																	</td>
																</tr>
																<tr>
																	<td>
																	</td>
																</tr>
																<% @pageData["events"]["data"].each do |event| %>
																	<tr>
																	<td valign="top">
																		<!--break-->
																		<table width="100%" border="0" cellspacing="0" cellpadding="0">
																			<tr>
																				<td height="40"></td>
																			</tr>
																		</table><!--/break-->
																		<p style="font-size: 18px; font-family: Georgia, 'Times New Roman', Times, serif; color: #333333; margin-top: 0px; margin-bottom: 0px; font-weight: bold;"><%= event["name"] %></p>
																		<p style="font-size: 18px; font-family: Georgia, 'Times New Roman', Times, serif; color: #606060; margin-top: 0px; margin-bottom: 0px;"><%= event["start_time"] %></p>
																		<table width="100%" border="0" cellpadding="0" cellspacing="0">
																			<tr>
																				<td height="10"></td>
																			</tr>
																		</table>
																		<p style="font-size: 16px; line-height: 18px; font-family: Georgia, 'Times New Roman', Times, serif; color: #333; margin: 0px;"><%= event["location"] %></p>

	<% if (@pageData["thatsme"]) %>
		Testing injecting some data through template_parser.rb

		<%= @pageData["thatsme"] %>
		<br />


	<% end %>
	<% @pageData["feed"]["data"].each do |feed| %>
		<%= feed["message"] %>
		<br />
	<% end %>

	<% @pageData["events"]["data"].each do |event| %>

		<% if event["description"] %>
			<%= event["description"] %> 
		<% end %>
	<% end %>
<% end %>

